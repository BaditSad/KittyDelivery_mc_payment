<section class="deliveries">
    <h2>Mes livraisons</h2>
    <% deliveries.forEach(delivery => { %>
      <div class="delivery">
        <h3>Livraison de <%= delivery.from %> à <%= delivery.to %></h3>
        <p>Commande : <%= delivery.orderNumber %></p>
        <p>Prix de la livraison : <%= delivery.price %> €</p>
        <div class="buttons">
          <button>Accepter</button>
          <button>Refuser</button>
        </div>
      </div>
    <% }) %>
  </section>
<aside class="profile">
    <h2>Mon profil</h2>
    <div class="profile-info">
        <!-- Profile information goes here -->
    </div>
    <button>Modifier</button>
    <button>Supprimer</button>
    <hr>
    <h2>Parrainage</h2>
    <label for="referral-email">Adresse mail à parrainer</label>
    <input type="email" id="referral-email">
    <button>Parrainer</button>
</aside>